---
const title = "Designer & Typographer";
const description =
  "Annie Szafranski is a California-based graphic designer and typographer designing editorial experiences across the web and in print.";

import "../styles/global.css";
import BaseLayout from "../layouts/BaseLayout.astro";
import IndexPhotoBrick from "../components/IndexPhotoBrick.astro";
---

<BaseLayout {title} {description}>
  <section class="about">
    <h1>
      Annie Szafranski is a graphic designer and typographer based in Southern California.
      She recently completed a year-long type design program, <a
      href="https://letterformarchive.org/type-west-online/"
      target="_blank">Type West</a
    >, through the Letterform Archive to enrich her design practice.
    </h1>
  </section>

  <section class="masonry">

    <div class="m-item item1">

        <a href="/work/primitiv-text">
            <IndexPhotoBrick fileName="Primitiv-Text_thumb_gc4qrs.jpg" alt="low res dark textured background from an old CRT TV with white type embedded into texture showing: Primitiv Text at an angle" />
         </a>
      
        <h2 class="project-title">
          <a href="work/primitiv-text">Primitiv Text</a>
        </h2>
        <p class="project-type">Type Design</p>

    </div>


    <div class="m-item item2">
        
        <a href="/work/beatrice-no-30-type-revival">
            <IndexPhotoBrick fileName="beatrice-no-30_1_s1aaoh.webp" alt="snippet of an illustration by Gustave Dore from Dante's Divine Comedy featuring Beatrice, with an olive green overlay and Beatrice No. 30 written in a white serif font in the center." />
           </a>
          <h2 class="project-title">
            <a href="work/beatrice-no-30-type-revival">Beatrice No. 30</a>
          </h2>
          <p class="project-type">Type Design</p>
</div>
    

    <div class="m-item item3">
        <a href="/work/the-guide-to-product-analytics-book-design"
        ><img
          src="/images/mixpanel-guide-to-product-analytics/guide-to-product-analytics-book.jpg" alt="a photograph of 3 copies of the same book, lined up at an angle and laying flat on a black background. The book is titled: The Guide to Product Analytics. It is a black hardcover book with metallic silver foil debossing"
        /></a
      >
      <h2 class="project-title">
        <a href="work/the-guide-to-product-analytics-book-design"
          >The Guide to Product Analytics</a>
      </h2>
      <p class="project-type">Book Design</p>
    </div>


    <div class="m-item item4">
        
        <a href="/work/april-webtape"
        ><img
          src="https://res.cloudinary.com/dzv7ytxjh/image/upload/v1738967076/april-webtape-1_msdg4j.jpg" alt="A photograph of a CD standing up, front and center, in front of a bright white background. The CD cover is type heavy, with layers of transparency hiding different descriptions and credits of the CD itself. The album is called 'Webtape' by the band, April."
        /></a
      >
      <h2 class="project-title">
        <a href="work/april-webtape">April: Webtape</a>
      </h2>
      <p class="project-type">Album Design</p>
  </div>
  
  
  <div class="m-item item5">
    <a href="/work/taxes-retirement-home-album-design"
    ><img
      src="/images/taxes-retirement-home/taxes-retirement-home-album_1.jpg" alt="Photo of the CD angled at a distance on a back background. The CD cover has a painting of a ghost on it with the band name somewhat hidden in the fabric of the ghost"
    /></a
  >
  <h2 class="project-title">
    <a href="work/april-webtape">Taxes: Retirement Home</a>
  </h2>
  <p class="project-type">Album Design</p>
  </div>


    <div class="m-item item6">
        <a href="/work/marlborough-titling">
            <IndexPhotoBrick fileName="marlborought-titling-thumb__jowip0.jpg" alt="A plain bright vermillion color background with creme text on top in sans serif type stating: Introducing Marlborough Titling (centered)." />
          </a>
          <h2 class="project-title">
            <a href="work/marlborough-titling">Marlborough Titling</a>
          </h2>
          <p class="project-type">Type Design</p>
    </div>
    
    <div class="m-item item7">Item 7</div>
    <div class="m-item item8">Item 8</div>

</section>

<!---------- TYPE PROJECT 1 ------------>




  </section>
</BaseLayout>
